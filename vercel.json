{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/alltoys",
        "dest": "/index.js"
      },
      {
        "src": "/alltoys/:id",
        "dest": "/index.js"
      },
      {
        "src": "/mytoys",
        "dest": "/index.js"
      },
      {
        "src": "/mytoys/:id",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  